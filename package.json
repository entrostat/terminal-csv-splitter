{
    "name": "terminal-csv-splitter",
    "description": "A CSV splitter that uses inbuilt terminal commands",
    "version": "1.0.1",
    "author": "Kerren @Kerren-Entrostat",
    "bin": {
        "terminal-csv-splitter": "./bin/run"
    },
    "bugs": "https://github.com/entrostat/terminal-csv-splitter/issues",
    "dependencies": {
        "@oclif/command": "^1.8.0",
        "@oclif/config": "^1.17.0",
        "@oclif/plugin-help": "^3.2.1",
        "fs-extra": "^9.1.0",
        "promax": "^2.0.2",
        "tslib": "^1.14.1",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1.26.0",
        "@oclif/test": "^1.2.8",
        "@types/chai": "^4.2.14",
        "@types/mocha": "^5.2.7",
        "@types/node": "^10.17.51",
        "@types/uuid": "^8.3.0",
        "chai": "^4.2.0",
        "eslint": "^5.16.0",
        "eslint-config-oclif": "^3.1.0",
        "eslint-config-oclif-typescript": "^0.1.0",
        "mocha": "^5.2.0",
        "nyc": "^14.1.1",
        "prettier": "^2.2.1",
        "standard-version": "^9.1.0",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.7"
    },
    "engines": {
        "node": ">=11.0.0"
    },
    "files": [
        "/bin",
        "/lib"
    ],
    "homepage": "https://github.com/entrostat/terminal-csv-splitter",
    "keywords": [
        "oclif"
    ],
    "license": "MIT",
    "main": "lib/index.js",
    "oclif": {
        "bin": "terminal-csv-splitter"
    },
    "repository": "entrostat/terminal-csv-splitter",
    "scripts": {
        "posttest": "eslint . --ext .ts --config .eslintrc",
        "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
        "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
        "version": "oclif-dev readme && git add README.md",
        "release": "standard-version --sign",
        "release:no-sign": "standard-version",
        "release:dry-run": "standard-version --dry-run"
    },
    "types": "lib/index.d.ts"
}
